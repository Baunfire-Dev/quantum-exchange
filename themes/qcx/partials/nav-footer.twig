				<div class="footer-form">
					{% if footer_form_label %}
						<p>{{ footer_form_label }}</p>
					{% endif %}

					<div class="footer-form-inner">
						<input type="text" placeholder="Email">

						<button>
							<svg width="40" height="40" viewbox="0 0 40 40" fill="none" xmlns="http://www.w3.org/2000/svg">
								<rect x="0.5" y="0.5" width="39" height="39" rx="3.5" fill="#206EEC"/>
								<rect x="0.5" y="0.5" width="39" height="39" rx="3.5" stroke="#206EEC"/>
								<path d="M17.6614 26L17 25.2567L21.6772 20L17 14.7433L17.6614 14L23 20L17.6614 26Z" fill="white"/>
							</svg>
						</button>
					</div>
				</div>
			</div>

			{% if primary_footer_nav_column|length %}
				<div class="footer-columns">
					{% for nav_column in primary_footer_nav_column %}
						<div class="footer-column">
							{% for nav_item in nav_column.primary_footer_nav_item %}
								<div class="footer-group">
									{% if nav_item.nav_item_title %}
										<div class="footer-group-head">
											<p class="button">{{ nav_item.nav_item_title }}</p>
										</div>
									{% endif %}

									{% if nav_item.child_nav_item|length %}
										<div class="footer-items">
											{% for child in nav_item.child_nav_item %}
												{% if child.link %}
													{% set link_url = child.link.url ?: "#" %}
													{% set link_title = child.link.title %}
													{% set link_target = child.link.target ?: "_self" %}

													<a class="footer-item uline uline-light" href="{{ link_url }}" target="{{ link_target }}">
														<p class="sm">{{ link_title }}</p>
													</a>
												{% endif %}
											{% endfor %}
										</div>
									{% endif %}
								</div>
							{% endfor %}
						</div>
					{% endfor %}
				</div>
			{% endif %}
		</div>
	</div>

	<div class="container">
		{% if footer_social|length or secondary_footer_nav_item|length %}
			<div class="footer-foot">
				{% if footer_social|length %}
					<div class="footer-socials">
						{% for social in footer_social %}
							{% set social_icon = social.type %}
							{% set social_link = social.link ?: "#" %}

							<a href="{{ social_link|lower }}" target="_blank" class="footer-social" rel="nofollow noreferrer noopener">
								<img src="{{ function("svg", social_icon) }}" alt="{{ social_icon }}" loading="lazy">
							</a>
						{% endfor %}
					</div>
				{% endif %}

				<div class="footer-foot-inner">
					{% if footer_credits %}
						<p class="xsm footer-credits">{{ function('do_shortcode', footer_credits) }}</p>
					{% endif %}

					{% if secondary_footer_nav_item|length %}
						<div class="footer-sub-items">
							{% for nav_item in secondary_footer_nav_item %}
								{% if nav_item.link %}
									{% set link_url = nav_item.link.url ?: "#" %}
									{% set link_title = nav_item.link.title %}
									{% set link_target = nav_item.link.target ?: "_self" %}

									<a class="footer-item secondary uline uline-light" href="{{ link_url }}" target="{{ link_target }}">
										<p class="xsm">{{ link_title }}</p>
									</a>
								{% endif %}
							{% endfor %}
						</div>
					{% endif %}
				</div>
			</div>
		{% endif %}
	</div>
</footer>
